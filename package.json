    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-helmet": "^5.1.3",
    "react-primitives": "^0.4.2",
    "react-responsive": "^1.3.0",
    "react-router": "^3.0.5",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2"
  },
  "babel": {
    "presets": [
      "babel-preset-env",
      "babel-preset-stage-1",
      "babel-preset-react"
    ],
    "plugins": [
      "babel-plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-i-am-meticulous/react",
      "eslint-config-i-am-meticulous/react-native"
    ],
    "env": {
      "es6": true
    },
    "rules": {
      "react/prop-types": 0
    }
  },
  "phenomic": {
    "presets": [
