  "repository": {
    "type": "git",
    "url": "git+https://github.com/syndia/phenomic-getting-started.git"
  },
  "keywords": [
    "phenomic"
  ],
  "author": "John Ripke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syndia/phenomic-getting-started/issues"
  },
  "homepage": "https://github.com/syndia/phenomic-getting-started#readme",
  "devDependencies": {
    "@phenomic/cli": "^1.0.0-alpha.3",
    "@phenomic/core": "^1.0.0-alpha.3",
    "@phenomic/preset-react-app": "^1.0.0-alpha.3",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^4.0.0",
    "eslint-config-i-am-meticulous": "^6.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.1.0",
